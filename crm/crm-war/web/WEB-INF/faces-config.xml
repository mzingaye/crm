<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>user</managed-bean-name>
        <managed-bean-class>controllers.UsersController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>catholic</managed-bean-name>
        <managed-bean-class>controllers.CatholicController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>sponsor</managed-bean-name>
        <managed-bean-class>controllers.SponsorController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>minister</managed-bean-name>
        <managed-bean-class>controllers.MinisterController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>baptismActions</managed-bean-name>
        <managed-bean-class>controllers.BaptismActions</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>confirmation</managed-bean-name>
        <managed-bean-class>controllers.ConfirmationController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>matrimonial</managed-bean-name>
        <managed-bean-class>controllers.MatrimonialController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>deathRec</managed-bean-name>
        <managed-bean-class>controllers.DeathRecController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userLogsController</managed-bean-name>
        <managed-bean-class>controllers.UserLogsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>reports</managed-bean-name>
        <managed-bean-class>controllers.ReportsController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>misc</managed-bean-name>
        <managed-bean-class>controllers.MiscController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
